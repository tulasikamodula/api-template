<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:munit="http://www.mulesoft.org/schema/mule/munit"
	xmlns:munit-tools="http://www.mulesoft.org/schema/mule/munit-tools"
	xmlns="http://www.mulesoft.org/schema/mule/core"
	xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xsi:schemaLocation="
		http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
		http://www.mulesoft.org/schema/mule/munit http://www.mulesoft.org/schema/mule/munit/current/mule-munit.xsd
		http://www.mulesoft.org/schema/mule/munit-tools  http://www.mulesoft.org/schema/mule/munit-tools/current/mule-munit-tools.xsd">
	<munit:config name="ping-test-suite.xml" ignore="false"/>
	<munit:test name="ping-test-suite_getHealthTest"
		doc:id="37e96aa1-b733-4961-900b-9718e7ab7585"
		description="Get Health Test">
		<munit:execution>
			<!-- <flow-ref doc:name="get:\ping:customers-prc-api-config"
				doc:id="797814b7-9661-4ebb-8a3f-979e491e4aa1"
				name="get:\ping:customers-prc-api-config" /> -->
		</munit:execution>
		<munit:validation>
			<munit-tools:assert-equals
				doc:name="Assert 200 Response"
				doc:id="ce7afc0d-df11-4376-93ab-202dfa60d597"
				actual="#[vars.httpStatus]" expected="200"
				message="Expected success response" />
		</munit:validation>
	</munit:test>
	<munit:test
		name="ping-test-suite_getHealthWithDependenciesTest"
		doc:id="6aa3b8a5-1309-4d57-80ee-62f33d484143"
		description="Get Health With Dependencies Test">
		<munit:behavior>
			<munit-tools:mock-when
				doc:name="Mock HTTP GET for System API"
				doc:id="a5f0435e-7239-4267-85a5-0b3ec2ffb376"
				processor="http:request">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute
						whereValue="GET" attributeName="method" />
				</munit-tools:with-attributes>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution>
			<munit:set-event doc:name="Set Query Param"
				doc:id="bee6d7ac-54ee-4f77-b9e6-34fdfc08ab55">
				<munit:attributes
					value='#[{ queryParams: { checkDependencies: "true" }}]' />
			</munit:set-event>
			<!-- <flow-ref doc:name="get:\ping:customers-prc-api-config"
				doc:id="13c62116-bcdd-4d79-a670-7fa5bcf407ad"
				name="get:\ping:customers-prc-api-config" /> -->
		</munit:execution>
		<munit:validation>
			<munit-tools:assert-equals
				doc:name="Assert 200 Response"
				doc:id="aa1ee847-476a-459b-ad70-5ea7bc50f4da"
				actual="#[vars.httpStatus]" expected="200"
				message="Expected success response" />
			<munit-tools:assert-that
				doc:name="Assert Has Dependencies"
				doc:id="a937e083-220d-444b-bf76-c6ed8fa2f4f6"
				expression="#[payload.dependencies]"
				is="#[MunitTools::notNullValue()]" />
		</munit:validation>
	</munit:test>
	<munit:test name="ping-test-suite_getHealthBadParamTest"
		doc:id="3e7031d3-95bb-48f9-867b-88e8d37a69d6"
		description="Get Health Bad Param Test">
		<munit:behavior>
			<munit-tools:mock-when
				doc:name="Mock HTTP GET for System API"
				doc:id="def5f6f6-07a0-45f2-b1ef-d18bc6105706"
				processor="http:request">
				<munit-tools:with-attributes>
					<munit-tools:with-attribute
						whereValue="GET" attributeName="method" />
				</munit-tools:with-attributes>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution>
			<munit:set-event doc:name="Set Query Param"
				doc:id="c392c83e-784a-4076-b044-99a5d7e9eb15">
				<munit:attributes
					value='#[{ queryParams: { checkDependencies: "true-ish" }}]' />
			</munit:set-event>
			<!-- <flow-ref doc:name="get:\ping:customers-prc-api-config"
				doc:id="b7333592-cad7-48a1-8e39-cee4e14a6f00"
				name="get:\ping:customers-prc-api-config" /> -->
		</munit:execution>
		<munit:validation>
			<munit-tools:assert-equals
				doc:name="Assert 200 Response"
				doc:id="b8cc64e1-67fd-4d67-a48f-2753ce21b28e"
				actual="#[vars.httpStatus]" expected="200"
				message="Expected success response" />
			<munit-tools:assert-that
				doc:name="Assert No Dependencies"
				doc:id="7acb1241-8dc5-4f28-ba10-b04879234eb3"
				expression="#[payload.dependencies]" is="#[MunitTools::nullValue()]" />
		</munit:validation>
	</munit:test>
</mule>
